<div class="title-section">
  <h5>Información del Contacto</h5>
</div>
<div class="field form-group ">
  <input type="text" name="firstname" id="9" class="input-big " placeholder="Nombre Contacto" data-required="" required="" minlength="5" maxlength="60" data-validate-regex="^[a-zA-Z0-9 ]{5,60}$">
  <label title="Nombre Contacto" for="contactname" data-title="Nombre Contacto"></label>
</div>

<div class="field form-group ">
  <input type="text" name="lastname" id="6" class="input-big " placeholder="Apellido" data-required="" required="" minlength="5" maxlength="60" data-validate-regex="^[a-zA-Z0-9 ]{5,60}$">
  <label title="Apellido" for="surname" data-title="Apellido"></label>
</div>

<div class="input-group prefix">
  <span class="input-group-addon">+593</span>
  <div class="field form-group ">
    <input type="text" name="mobilephone" id="10" class="input-phone   " placeholder="Teléfono Móvil" data-required="" required="" minlength="7" maxlength="7" data-validate-regex="^[0-9]{7,7}$">

    <label title="Teléfono Móvil" for="telephone2" data-title="Teléfono Móvil"></label>
    <span class="error-message"></span>
  </div>
</div>



